prefix=/opt/homebrew/Cellar/vips/8.18.0
includedir=${prefix}/include
libdir=${prefix}/lib

Name: vips-cpp
Description: C++ API for vips8 image processing library
Version: 8.18.0
Requires: vips, glib-2.0 >= 2.52, gobject-2.0
Requires.private: gio-2.0, gmodule-no-export-2.0, expat, zlib >= 0.4, /opt/homebrew/opt/libarchive/lib/pkgconfig/libarchive.pc, fftw3, cfitsio, imagequant, cgif >= 0.2.0, libexif >= 0.6, libjpeg, libuhdr, libpng >= 1.2.9, libwebp >= 0.6, libwebpmux >= 0.6, libwebpdemux >= 0.6, pangocairo >= 1.32.6, pangoft2 >= 1.32.6, fontconfig, libtiff-4, librsvg-2.0 >= 2.40.3, cairo >= 1.2, matio, lcms2, OpenEXR >= 1.2.2, libraw_r >= 0.14, libopenjp2 >= 2.4, libhwy >= 1.0.5
Libs: -L${libdir} -lvips-cpp
Libs.private: -lm -lintl
Cflags: -I${includedir}
